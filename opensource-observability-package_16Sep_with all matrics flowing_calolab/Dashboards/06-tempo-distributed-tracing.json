{
  "__inputs": [
    {
      "name": "DS_TEMPO",
      "label": "Tempo",
      "description": "",
      "type": "datasource",
      "pluginId": "tempo",
      "pluginName": "Tempo"
    },
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "For Tempo metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "8.0.0"
    },
    {
      "type": "panel",
      "id": "traces",
      "name": "Traces",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "nodeGraph",
      "name": "Node Graph",
      "version": ""
    },
    {
      "type": "datasource",
      "id": "tempo",
      "name": "Tempo",
      "version": "1.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "id": null,
  "title": "Tempo Distributed Tracing",
  "tags": ["tempo", "tracing", "distributed", "performance"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "schemaVersion": 30,
  "version": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": "${DS_TEMPO}",
        "query": "search",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "hide": 0,
        "label": "Service",
        "options": [],
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Tempo Service Status",
      "type": "stat",
      "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "up{job=\"tempo\"}",
          "legendFormat": "Tempo Service",
          "refId": "A",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "expr": "tempo_ingester_blocks_flushed_total",
          "legendFormat": "Blocks Flushed",
          "refId": "B",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "expr": "tempo_distributor_spans_received_total",
          "legendFormat": "Spans Received",
          "refId": "C",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "expr": "tempo_query_frontend_queries_total",
          "legendFormat": "Queries Total",
          "refId": "D",
          "datasource": "${DS_PROMETHEUS}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "background"
      }
    },
    {
      "id": 2,
      "title": "Trace Search & Analysis",
      "type": "traces",
      "gridPos": {"h": 16, "w": 24, "x": 0, "y": 6},
      "targets": [
        {
          "refId": "A",
          "datasource": "${DS_TEMPO}",
          "query": "{.service.name=\"$service\"}",
          "queryType": "traceql"
        }
      ],
      "options": {
        "nodeGraph": {
          "enabled": true
        }
      }
    },
    {
      "id": 3,
      "title": "Spans Received Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
      "targets": [
        {
          "expr": "rate(tempo_distributor_spans_received_total[5m])",
          "legendFormat": "{{instance}} - Spans/sec",
          "refId": "A",
          "datasource": "${DS_PROMETHEUS}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 4,
      "title": "Query Performance",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(tempo_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "A",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "expr": "histogram_quantile(0.50, rate(tempo_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "B",
          "datasource": "${DS_PROMETHEUS}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 5,
      "title": "Tempo Storage Metrics",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
      "targets": [
        {
          "expr": "tempo_ingester_blocks_flushed_total",
          "legendFormat": "Blocks Flushed",
          "refId": "A",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "expr": "tempo_ingester_traces_created_total",
          "legendFormat": "Traces Created",
          "refId": "B",
          "datasource": "${DS_PROMETHEUS}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 6,
      "title": "Error Rates",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
      "targets": [
        {
          "expr": "rate(tempo_request_duration_seconds_count{status_code!~\"2..\"}[5m])",
          "legendFormat": "Error Rate",
          "refId": "A",
          "datasource": "${DS_PROMETHEUS}"
        },
        {
          "expr": "rate(tempo_request_duration_seconds_count[5m])",
          "legendFormat": "Total Requests",
          "refId": "B",
          "datasource": "${DS_PROMETHEUS}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 7,
      "title": "Service Map & Dependencies",
      "type": "nodeGraph",
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 38},
      "targets": [
        {
          "refId": "A",
          "datasource": "${DS_TEMPO}",
          "query": "{.service.name=~\".+\"}",
          "queryType": "traceql"
        }
      ],
      "options": {
        "nodes": {
          "mainStatUnit": "ms",
          "secondaryStatUnit": "ops"
        },
        "edges": {
          "mainStatUnit": "ms",
          "secondaryStatUnit": "ops"
        }
      }
    },
    {
      "id": 8,
      "title": "Recent High-Latency Traces",
      "type": "traces",
      "gridPos": {"h": 12, "w": 24, "x": 0, "y": 50},
      "targets": [
        {
          "refId": "A",
          "datasource": "${DS_TEMPO}",
          "query": "{duration > 1s}",
          "queryType": "traceql"
        }
      ],
      "options": {
        "nodeGraph": {
          "enabled": false
        }
      }
    }
  ]
}